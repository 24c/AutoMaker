<?xml version="1.0" encoding="UTF-8"?>
<root>
    <ApplicationConfiguration>
        <ApplicationName>CEL AutoMaker</ApplicationName>
        <ApplicationDataStorageDirectory>$APPDATA_HOME$/#ApplicationConfiguration:ApplicationName#/</ApplicationDataStorageDirectory>
        <UserDataStorageDirectory>$USER_HOME$/#ApplicationConfiguration:ApplicationName#/</UserDataStorageDirectory>
        <requiredFirmwareVersion>635</requiredFirmwareVersion>
        <DefaultMode>Normal</DefaultMode>
        <DefaultLocaleLanguageTag>en-GB</DefaultLocaleLanguageTag>
        <FakeInstallDirectory>C:\Program Files\CEL\AutoMaker\</FakeInstallDirectory>
    </ApplicationConfiguration>
    <Stenographer>
        <logfilename>#ApplicationConfiguration:ApplicationDataStorageDirectory#/AutoMaker.log</logfilename>
        <logmode>LOCAL</logmode>
        <logtee>T</logtee>
        <loglevel>TRACE</loglevel>
    </Stenographer>
</root>